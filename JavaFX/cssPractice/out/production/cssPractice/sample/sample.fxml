<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.web.WebView?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10"
          stylesheets="@styles.css" fx:id="gridPane">

    <Button text="Open..." onAction="#fileChooser" GridPane.rowIndex="0" GridPane.columnIndex="1" style="-fx-background-color: green"/>
    <Button text="Button Two" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
    <Button text="Button Three" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <Button text="Button Four" fx:id="button4" GridPane.rowIndex="0" GridPane.columnIndex="4"/>
    <Button text="Button Five" id="idButtonFive" GridPane.rowIndex="0" GridPane.columnIndex="5"/>
    <CheckBox text="Check box" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
    <Label text="JavaFX Effects" fx:id="label" onMouseEntered="#onMouseEntered" onMouseExited="#onMouseExited" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
<!--    <Hyperlink text="sdfasdf" GridPane.rowIndex="2" GridPane.columnIndex="0"/>-->
<!--    <WebView GridPane.rowIndex="3" GridPane.columnIndex="0" GridPane.columnSpan="5" GridPane.rowSpan="3"/>-->
</GridPane>